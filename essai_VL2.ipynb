    "import numpy as np\n",
    "\n",
    "def cube(n):\n",
    "    return n**3\n",
    "        \n",
    "def volume_sphere(R):\n",
    "    return 4 / 3 * np.pi * cube(R)\n",
    "\n",
    "r = float(input(\"quel est le rayon\"))\n",
    "print(\"Le volume de cette sphere vaut\", volume_sphere(r))"
   ]
